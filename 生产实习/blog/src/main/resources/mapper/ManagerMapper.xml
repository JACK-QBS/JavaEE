<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liu.dao.ManagerMapper">

    <select id="selectManagerByNameAndPassword" parameterType="com.liu.model.Manager" resultType="com.liu.model.Manager">
        select * from t_manager where username = #{username} and password = #{password}
    </select>
</mapper>